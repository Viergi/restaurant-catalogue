{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "quotes": "off",
    "no-use-before-define": "off",
    "no-underscore-dangle": "off",
    "comma-dangle": "off",
    "operator-linebreak": "off",
    "no-console": "off",
    "no-alert": "off",
    "eqeqeq": "off",
    "dot-notation": "off",
    "no-unused-vars": "off",
    "indent": "off",
    "arrow-body-style": "off",
    "no-nested-ternary": "off",
    "no-restricted-globals": "off",
    "eslint-plugin-import/no-extraneous-dependencies": "off",
    "implicit-arrow-linebreak": "off"
  }
}
